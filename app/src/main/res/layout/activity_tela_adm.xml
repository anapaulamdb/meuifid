<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".activityViews.TelaAdm">


    <ImageView
        android:id="@+id/logo"
        android:layout_width="188dp"
        android:layout_height="96dp"
        android:src="@drawable/logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />


    <TextView
        android:id="@+id/categorias"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:hint="Categoria"
        android:textColor="@color/white"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="0dp" />

    <View
        android:id="@+id/containerCard_ADM"
        style="@style/ContCard"
        android:layout_width="380dp"
        android:layout_height="166dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/categorias"
        app:layout_constraintVertical_bias="0.219" />

    <ImageView
        android:id="@+id/ft_adm"
        android:layout_width="86dp"
        android:layout_height="81dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="40dp"
        android:src="@drawable/ic_user"
        app:layout_constraintBottom_toBottomOf="@+id/containerCard_ADM"
        app:layout_constraintEnd_toEndOf="@+id/containerCard_ADM"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/containerCard_ADM"
        app:layout_constraintVertical_bias="0.244" />

    <TextView
        android:id="@+id/titulo_card"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ADMINISTRADOR"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@id/containerCard_ADM"
        app:layout_constraintEnd_toEndOf="@id/containerCard_ADM"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="@id/containerCard_ADM"
        app:layout_constraintTop_toTopOf="@id/containerCard_ADM"
        app:layout_constraintVertical_bias="0.093" />

    <TextView
        android:id="@+id/txtNome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="176dp"
        android:text="Nome"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/containerCard_ADM"
        app:layout_constraintEnd_toEndOf="@+id/containerCard_ADM"
        app:layout_constraintLeft_toLeftOf="@id/containerCard_ADM"
        app:layout_constraintTop_toTopOf="@id/containerCard_ADM"
        app:layout_constraintVertical_bias="0.435" />

    <TextView
        android:id="@+id/textNome"
        android:layout_width="158dp"
        android:layout_height="17dp"
        android:text=" "
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/containerCard_ADM"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintHorizontal_bias="0.691"
        app:layout_constraintLeft_toLeftOf="@id/containerCard_ADM"
        app:layout_constraintStart_toStartOf="@+id/containerCard_ADM"
        app:layout_constraintTop_toTopOf="@id/containerCard_ADM"
        app:layout_constraintVertical_bias="0.557" />

    <TextView
        android:id="@+id/txtCargo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="176dp"
        android:text="Cargo"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/containerCard_ADM"
        app:layout_constraintEnd_toEndOf="@+id/containerCard_ADM"
        app:layout_constraintLeft_toLeftOf="@id/containerCard_ADM"
        app:layout_constraintTop_toTopOf="@id/containerCard_ADM"
        app:layout_constraintVertical_bias="0.768" />

    <TextView
        android:id="@+id/txtCargo_"
        android:layout_width="184dp"
        android:layout_height="18dp"
        android:text=" "
        android:textColor="@color/black"

        app:layout_constraintBottom_toBottomOf="@+id/containerCard_ADM"
        app:layout_constraintEnd_toEndOf="@+id/containerCard_ADM"
        app:layout_constraintHorizontal_bias="0.826"
        app:layout_constraintStart_toStartOf="@+id/containerCard_ADM"
        app:layout_constraintTop_toBottomOf="@+id/textNome"
        app:layout_constraintVertical_bias="0.754" />

    <View
        android:id="@+id/containerComponents_CAT"
        style="@style/Editor_txt"
        android:layout_width="391dp"
        android:layout_height="364dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.582"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logo"
        app:layout_constraintVertical_bias="0.869" />

    <ImageView
        android:id="@+id/sair"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/logout"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.824"
        app:layout_constraintStart_toEndOf="@+id/logo"
        app:layout_constraintTop_toTopOf="@+id/categorias"
        app:layout_constraintVertical_bias="0.112" />

    <TextView
        android:id="@+id/titulocard_ADM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CATEGORIAS"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="@+id/containerComponents_CAT"
        app:layout_constraintEnd_toEndOf="@+id/containerComponents_CAT"
        app:layout_constraintStart_toStartOf="@+id/containerComponents_CAT"
        app:layout_constraintTop_toTopOf="@+id/containerComponents_CAT"
        app:layout_constraintVertical_bias="0.086" />

    <TextView
        android:id="@+id/aprovados_adm"
        style="@style/Editor_txt"
        android:layout_width="316dp"
        android:layout_height="75dp"
        android:gravity="center"
        android:drawableRight="@drawable/ic_baseline_check_24"
        android:text="APROVADOS"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/containerComponents_CAT"
        app:layout_constraintEnd_toEndOf="@+id/containerComponents_CAT"
        app:layout_constraintHorizontal_bias="0.258"
        app:layout_constraintStart_toStartOf="@+id/containerComponents_CAT"
        app:layout_constraintTop_toTopOf="@+id/containerComponents_CAT"
        app:layout_constraintVertical_bias="0.302" />

    <TextView
        android:id="@+id/pendente_adm"
        style="@style/Editor_txt"
        android:layout_width="316dp"
        android:layout_height="75dp"
        android:gravity="center"
        android:drawableRight="@drawable/ic_pendente"
        android:text="PENDENTES"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/containerComponents_CAT"
        app:layout_constraintEnd_toEndOf="@+id/containerComponents_CAT"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="@+id/containerComponents_CAT"
        app:layout_constraintTop_toBottomOf="@+id/aprovados_adm"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/expirados_adm"
        style="@style/Editor_txt"
        android:layout_width="316dp"
        android:layout_height="75dp"
        android:gravity="center"
        android:drawableRight="@drawable/ic_block"
        android:text="EXPIRADOS"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/containerComponents_CAT"
        app:layout_constraintEnd_toEndOf="@+id/containerComponents_CAT"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="@+id/containerComponents_CAT"
        app:layout_constraintTop_toBottomOf="@+id/pendente_adm"
        app:layout_constraintVertical_bias="0.0" />


</androidx.constraintlayout.widget.ConstraintLayout>